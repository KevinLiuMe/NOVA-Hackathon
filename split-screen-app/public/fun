(* Task 2.1 *)
val task2_1 = (15150 :: []) :: []

(* Task 2.2 *)
val task2_2 = fn [] => true | x :: xs => xs

(* Task 2.3 *)
val task2_3 = fn [] => true | x :: xs => x

(* Task 2.4 *)
val task2_4 = ( let
    fun length ([]: int list) : int = 0
      | length (x :: xs) = 1 + length(xs)
    in
    length [1, 5, 1, 5, 0]
    end ) + 1

(* Task 2.5 *)
val task2_5 = fn (x : int) => [x, 1 + 1]
